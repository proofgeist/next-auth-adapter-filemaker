{
    "compilerOptions": {
        "strictNullChecks": true,
        "target": "es2019",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "declaration": true,
        "stripInternal": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true
    }
}